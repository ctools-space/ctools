{
    "name": "electron-vue",
    "version": "0.0.1",
    "main": "dist/main/index.js",
    "license": "MIT",
    "scripts": {
        "dev": "cross-env NODE_ENV=development DEPLOY_ENV=development concurrently -c green,blue,yellow 'npm run dev:*'",
        "dev:vue": "vite",
        "dev:ele": "ts-node script/build --env=development --watch",
        "dev:plugins": "vite -c vite-plugins.config.ts",
        "build": "rm -rf dist && npm run build:vue && npm run build:ele && electron-builder",
        "build:vue": "vite build",
        "build:ele": "ts-node script/build --env=production",
        "build:plugins": "vite build -c vite-plugins.config.ts",
        "dump": "HUSKY=0 standard-version --release-as",
        "release": "npm publish",
        "rebuild": "electron-rebuild"
    },
    "dependencies": {
        "@pokemonon/knife": "^1.0.0-beta.8",
        "@quasar/extras": "^1.10.10",
        "bplist-parser": "^0.3.0",
        "bufferutil": "^4.0.3",
        "electron-better-ipc": "^1.1.1",
        "element-plus": "^1.0.1-beta.27",
        "execa": "^5.0.0",
        "exit-hook": "^2.2.0",
        "fuzzyjs": "^5.0.1",
        "get-port": "^5.1.1",
        "globby": "^11.0.4",
        "hotkeys-js": "^3.8.7",
        "iohook": "^0.9.3",
        "plist": "^3.0.3",
        "quasar": "^2.0.3",
        "request-ip": "^2.1.3",
        "robotjs": "^0.6.0",
        "simple-plist": "^1.1.1",
        "tailwindcss": "^2.2.7",
        "utf-8-validate": "^5.0.4",
        "vue": "^3.0.0",
        "vue-router": "4",
        "vuex": "^4.0.2",
        "whistle": "^2.6.3",
        "ws": "^7.4.3"
    },
    "devDependencies": {
        "@pokemonon/eslint-plugin-common": "~0.0.2",
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@types/minimist": "^1.2.1",
        "@types/plist": "^3.0.2",
        "@types/wait-on": "^5.2.0",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "@vitejs/plugin-vue": "^1.0.6",
        "@vitejs/plugin-vue-jsx": "^1.0.2",
        "@vue/compiler-sfc": "^3.0.0",
        "chalk": "^4.1.0",
        "commitizen": "^4.2.4",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv-expand": "^5.1.0",
        "electron": "13.1.8",
        "electron-abi": "^1.1.0",
        "electron-builder": "^22.8.0",
        "electron-is-dev": "^1.2.0",
        "electron-rebuild": "^3.2.0",
        "electron-store": "^6.0.0",
        "eslint": "^7.27.0",
        "husky": "^6.0.0",
        "less": "^4.1.0",
        "less-loader": "^7.2.1",
        "minimist": "^1.2.5",
        "node-abi": "^2.30.0",
        "ora": "^5.0.0",
        "postcss-extend": "^1.0.5",
        "postcss-nested": "^5.0.6",
        "rollup": "^2.55.1",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-esbuild": "^4.5.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-externals": "^2.2.0",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-ts-paths": "^1.0.5",
        "rollup-plugin-typescript-paths": "^1.2.2",
        "standard-version": "^9.3.0",
        "tailwindcss-important": "^1.0.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.0.3",
        "vite": "^2.4.4",
        "vite-plugin-externals": "^0.1.5",
        "vite-plugin-resolve-externals": "^0.1.0",
        "vite-tsconfig-paths": "^3.3.13",
        "wait-on": "^5.2.0"
    },
    "keywords": [
        "vite",
        "electron",
        "vue3",
        "rollup"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "iohook": {
        "targets": [
            "electron-85"
        ],
        "platforms": [
            "darwin",
            "win32"
        ],
        "arches": [
            "x64",
            "ia32"
        ]
    }
}